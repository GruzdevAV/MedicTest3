<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_analysis">
    <fragment
        android:id="@+id/navigation_analysis"
        android:name="com.example.medictest3.ui.Analysis.AnalysisFragment"
        android:label="Анализы"
        tools:layout="@layout/fragment_analysis" />
    <fragment
        android:id="@+id/navigation_results"
        android:name="com.example.medictest3.ui.Results.ResultsFragment"
        android:label="Результаты"
        tools:layout="@layout/fragment_results" />
    <fragment
        android:id="@+id/supportFragment"
        android:name="com.example.medictest3.ui.Support.SupportFragment"
        android:label="fragment_support"
        tools:layout="@layout/fragment_support" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.medictest3.ui.Profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>